pkgname=@PROJECT_NAME@
pkgver=@PROJECT_VERSION@
pkgrel=1
pkgdesc="@PROJECT_DESCRIPTION@"
arch=('any')
license=('GPL')
depends=(gnuradio)
source=("$pkgname-$pkgver.tar.gz")
md5sums=('SKIP')
url="https://github.com/utat-ss/HERON-gr-utat"
build(){
  cd "$srcdir"
}
package() {
  cd "$srcdir"
  tar -xf "$pkgname-$pkgver.tar.gz" -C "$pkgdir"
  cd "$pkgdir"
  mv "$pkgname-$pkgver" "usr"
}